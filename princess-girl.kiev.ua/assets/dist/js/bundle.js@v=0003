	function getError(action, option, xhr) {
	    var msg = 'fail to post ' + action + ' ' + xhr.status + '\'';
	    var err = new Error(msg);
	    err.status = xhr.status;
	    err.method = 'post';
	    err.url = action;
	    return err;
	}
	
	function getBody(xhr) {
	    var text = xhr.responseText || xhr.response;
	    if (!text) {
	        return text;
	    }
	
	    try {
	        return JSON.parse(text);
	    } catch (e) {
